{
	"name": "DataflowEmp",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "EmployeeDS",
						"type": "DatasetReference"
					},
					"name": "EmpSource"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Age as short,",
				"          Gender as string,",
				"          MaritalStatus as string,",
				"          EmployeeNumber as string,",
				"          EmployeeName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> EmpSource",
				"EmpSource derive({ EmployeeName} = replace( EmployeeName, \",\", \" \")) ~> derivedColumn2"
			]
		}
	}
}