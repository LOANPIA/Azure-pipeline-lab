{
	"name": "DataflowEmp",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "EmployeeDS",
						"type": "DatasetReference"
					},
					"name": "EmployeeSource"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Age as short,",
				"          Gender as string,",
				"          MaritalStatus as string,",
				"          EmployeeNumber as string,",
				"          EmployeeName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> EmployeeSource",
				"derivedColumn2 derive() ~> derivedColumn1",
				"EmployeeSource derive() ~> derivedColumn2"
			]
		}
	}
}